	+header-classic

	+breadcrumbs({
		category: activeCategory,
		path: ['Services #2'],
		mod: 'bg-overlay-68',
		bgImage: 'bg-services'
	})

	// Section-services
	section.section.section-xl.bg-default
		.container
			.row.row-50.justify-content-center.box-ordered
				.col-md-6.col-lg-5.col-xl-4
