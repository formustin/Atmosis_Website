	// Our mission
	section.section.section-lg.section-inset-custom-4.context-dark.bg-overlay-4-60.bg-vide( id='1', data-vide-bg="video/video-1" data-vide-options='posterType: jpg, position: 50% 50%')
		.container: .row.justify-content-center: .col-lg-10.col-xl-8: .video-modern
