			.row.row-60.row-sm
				.col-sm-6.col-lg-4.wow.fadeInLeft
					+post-modern({
						img: 'landscape-design/post-30-370x307',
						time: { datetime: '2019-07-04', dateNumber: '04', dateMonth: 'July' },
						title: 'Top 5 best landscapes for summer',
						text: 'Annual survey and analysis of community of over 35 million monthly users reveals the top-rated home remodeling professionals...'
					})

				.col-sm-6.col-lg-4.wow.fadeInLeft( data-wow-delay='.1s' )
					+post-modern({
						img: 'landscape-design/post-31-370x307',
						time: { datetime: '2019-07-17', dateNumber: '17', dateMonth: 'July' },
						title: 'The Right Stones for your Garden',
						text: 'Stone has always been a cost-effective hardscape material for all garden styles. Nowadays they are used as an affordable...'
					})

				.col-sm-6.col-lg-4.wow.fadeInLeft( data-wow-delay='.2s' )
					+post-modern({
						img: 'landscape-design/post-32-370x307',
						time: { datetime: '2019-07-22', dateNumber: '22', dateMonth: 'July' },
						title: 'Why landscape design is important',
						text: 'It is vital to have the house look attractive from the outside, as it is the face of you and your family. In some way it gives a picture of the...'
