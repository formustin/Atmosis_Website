//- Contact information
.container
	.row.row-30.justify-content-center
		.col-sm-8.col-md-6.col-lg-4
			+box-contacts({
				icon: 'fl-bigmug-line-cellphone55',
				tel: [
					{ text: '+1 323-913-4688' },
					{ text: '+1 323-888-4554' }
				]
			})
		.col-sm-8.col-md-6.col-lg-4
			+box-contacts({
				icon: 'fl-bigmug-line-up104',
				address: [
					{ text: '4730 Crystal Springs Dr, Los Angeles, CA 90027' }
				]
			})
		.col-sm-8.col-md-6.col-lg-4
			+box-contacts({
				icon: 'fl-bigmug-line-chat55',
				mail: [
					{ text: 'mail@demolimk.org' },
					{ text: 'info@demolimk.org' }
				]
			})
